{
  "name": "delta-v-back",
  "version": "0.0.1",
  "main": "src/index.ts",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "build": "tsc -p ./tsconfig.build.json",
    "install": "sh ./build.sh",
    "lint": "eslint --ext .ts src/ tests/",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest",
    "jest": "jest -i --verbose",
    "jest:watch": "jest -i --verbose --watch",
    "ts-typeorm": "ts-node ./node_modules/.bin/typeorm --config ./src/loader/ormconfig.ts",
    "typeorm:drop": "yarn ts-typeorm schema:drop",
    "migration:generate": "yarn ts-typeorm migration:generate",
    "migration:run": "yarn ts-typeorm migration:run",
    "migration:revert": "yarn ts-typeorm migration:revert",
    "migration:create": "yarn ts-typeorm migration:create -d ./src/migrations/ -n ",
    "fixtures:load": "fixtures ./fixtures --config ./src/loader/ormconfig.ts --debug --require=ts-node/register",
    "cron:syncCurrencies": "node ./dist/scripts/syncCurrency"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "express": "^4.18.1",
    "express-pino-logger": "^7.0.0",
    "i18n-iso-countries": "^7.4.0",
    "joi": "^17.6.0",
    "jsonata": "^1.8.6",
    "lexorank": "1.0.4",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "redact-object": "^3.0.1",
    "sanitize-html": "^2.7.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "7.3.0",
    "@snowpact/eslint-config": "^1.3.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^27.4.0",
    "@types/node": "18.15.11",
    "@types/nodemailer": "^6.4.7",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "@types/sanitize-html": "^2.6.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "5.57.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.0",
    "jest-mock-axios": "^4.6.0",
    "nodemon": "^2.0.22",
    "prettier": "2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typeorm-fixtures-cli": "^1.11.1",
    "typescript": "^4.9.5"
  },
  "repository": "git@github.com:sabalot/test-circle-ci-clever-cloud.git",
  "author": "Sabalot <s.sabalot@gmail.com>",
  "license": "MIT"
}
