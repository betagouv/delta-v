{
  "name": "delta-v-back",
  "version": "0.0.1",
  "main": "src/index.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "build": "tsc -p ./tsconfig.build.json",
    "install": "sh ./build.sh",
    "lint": "eslint --ext .ts src/ tests/",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest",
    "jest": "jest -i --verbose",
    "jest:watch": "jest -i --verbose --watch",
    "typeorm:drop": "yarn typeorm-ts-node-commonjs schema:drop -d ./src/loader/ormconfig.ts",
    "migration:generate": "yarn typeorm-ts-node-commonjs migration:generate -d ./src/loader/ormconfig.ts -p ",
    "migration:run": "yarn typeorm-ts-node-commonjs migration:run -d ./src/loader/ormconfig.ts",
    "migration:revert": "yarn typeorm-ts-node-commonjs migration:revert -d ./src/loader/ormconfig.ts",
    "migration:create": "yarn typeorm-ts-node-commonjs migration:create -d ./src/loader/ormconfig.ts ",
    "fixtures:load": "fixtures ./fixtures --config ./src/loader/ormconfig.ts --debug --require=ts-node/register",
    "cron:syncCurrencies": "node ./dist/scripts/syncCurrency"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "bcrypt": "5.1.0",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "i18n-iso-countries": "^7.6.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "9.0.0",
    "lexorank": "1.0.5",
    "nodemailer": "6.9.1",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "redact-object": "^3.0.1",
    "sanitize-html": "^2.7.0",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@snowpact/eslint-config": "^1.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-pino-logger": "^4.0.3",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "18.16.0",
    "@types/nodemailer": "^6.4.7",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "@types/sanitize-html": "^2.6.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "jest-mock-axios": "^4.6.0",
    "nodemon": "^2.0.22",
    "prettier": "2.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.5.0",
    "typeorm-fixtures-cli": "^1.11.1",
    "typescript": "^5.0.4"
  },
  "repository": "git@github.com:sabalot/test-circle-ci-clever-cloud.git",
  "author": "Sabalot <s.sabalot@gmail.com>",
  "license": "MIT"
}
